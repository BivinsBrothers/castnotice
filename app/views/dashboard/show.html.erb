<h1>Dashboard</h1>

<% if current_user.resume %>
  <%= render 'resumes/close_up', resume: current_user.resume %>
  <%= render 'resumes/projects', resume: current_user.resume %>
  <%= render 'resumes/schools', resume: current_user.resume %>
<% else %>
  <%= link_to 'Create Resume', edit_resume_path %>
<% end %>
