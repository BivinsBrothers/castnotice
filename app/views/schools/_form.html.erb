<%= form_for @school do |f| %>
  <%= f.label :education_type %>
  <%= f.select :education_type, education_type_select %>
  <%= f.label :school %>
  <%= f.text_field :school %>
  <%= f.label :major %>
  <%= f.text_field :major %>
  <%= f.label :degree %>
  <%= f.text_field :degree %>
  <%= f.label :teacher %>
  <%= f.text_field :teacher %>
  <%= f.label :years %>
  <%= f.text_field :years %>
  <%= f.label :instruments %>
  <%= f.text_field :instruments %>
  <%= f.submit "Save" %>
<% end %>