/* CSS Document #FF2626 (red) #FFBF00 (yellow)*/
html {
  background-color:#000;
}
html, body{
	height:100%;
	width:100%;
	font-size:14px;
	color:#FFF;
	font-family: 'Open Sans', sans-serif;
}
body{
  background-color:transparent;
}
.container {
  margin-bottom: 50px;
}
h1, h2, h3, h4, h5, h6, p, li{
	-webkit-font-smoothing: antialiased;
	font-family: 'Open Sans', sans-serif;
}
h1{
	font-weight:300;
	color:#FFF;
	font-size:55px;
	line-height: 55px;
	margin:0 0 25px 0;
	padding:0;
}
h1.pricing{
	margin:0 0 10px 0;
}
h2{
	font-size: 22px;
	color: #FF2626;
	font-weight:300;
	padding-bottom:5px;
	margin-bottom:25px;
	border-bottom:1px solid #FF2626;
}
h3{
	font-size: 22px;
	color: #FFBF00;
	font-weight:300;
	margin-bottom: 20px;
}
h4{
	color:#fff;
	font-weight:bold;
	font-size:16px;
	line-height: 16px;
	margin: 1em 0 12px 0;
}
.glyphicon {
  top: 2px;
  color:#FF2626;
}
ol.how-it-works{
	counter-reset: li;
	list-style: none;
	padding-left:30px;
}
ol li {
	position: relative;
	display: block;
	padding: 0px 0 3px 20px;
	margin: 0 0 20px 0;
	font-size: 14px;
}
ol.how-it-works li:before {
	content: counter(li);
	counter-increment: li;
	position: absolute;
	left: -25px;
	top: 0;
	margin-top: 0;
	background: #FF2626;
	height: 30px;
	width: 30px;
	line-height: 25px;
	text-align: center;
	padding: 2px 2px 1px 0px;
	border-radius: 2em;
	font-size: 20px;
	color: #FFF;
}
/*Form Styles*/
.btn, a{
	-webkit-transition:0.8s all;
	transition:0.8s all;
}
input, select, textarea, input[type="checkbox"]{
	margin: 0 0 15px 0;
	color: #444;
	background-color:#EEE;
	font-family: 'Open Sans', sans-serif;
	border: none;
	border-radius: 7px;
	padding: 4px 20px 4px 7px;
	display: block;

}
select, input[type="text"]{
	height:27px;
	width:250px;
}
input:focus, textarea:focus, isindex:focus, keygen:focus, select:focus {
	outline:none;
	background-color:#FFF;
}

input[type="checkbox"]{
	margin: 0 0 10px 0;
	color: #444;
	background-color:#EEE;
	font-family: 'Open Sans', sans-serif;
	border: none;
	border-radius: 5px;
	padding: 3px 15px;
	display: inline;

}
input[type="file"]{
	background-color: transparent;
	display: inline;
	color:#fff;
	padding: 0;
	width: 250px;
}
input[type="textarea"], textarea{
	width: 100%;
	height: 100px;
}
.btn-primary, .btn, input[type="submit"] {
  	font-family: 'Open Sans', sans-serif;
	color:#fff;
	background-color: #FF2626;
	border: none;
	border-radius: 20px;
	padding: 10px 30px;
	margin: 15px 0 15px 0;
	display: inline-block;
}
.btn-small{
	font-size: 12px;
	padding: 2px 10px 3px 10px;
	margin: -4px 0 0 10px;
}
#btn-tiny{
  position:fixed;
  right: 165px;
  top: 5px;
  font-size: 12px;
  padding: 5px 15px;
}
.btn-add-new{
	font-size: 14px;
	color: #FF2626;
}
.btn-edit{
	font-size: 14px;
	color: #FF2626;
}
.btn:hover, .btn:focus, input[type="submit"]:hover, .btn-primary:hover {
	outline: none;
	background-color: #FFBF00;
	background-position: 0 -15px;
	border: none;
}

label {
	font-weight: normal;
	font-family: 'Open Sans', sans-serif;
}

.new_event label{
	width:auto;
	padding:0 15px 0 5px;
}
.is-hidden {
  display:none;
}

#user_birthday_1i, #user_birthday_2i, #user_birthday_3i{
	display:inline;
}
.chosen-container {
  display: block;
}
.chosen-container .chosen-choices {
	width: 100%;
	border-radius:7px;
	background-color:#EEE;
	font-family: 'Open Sans', sans-serif;
	border: none;
	padding: 4px 20px 4px 7px;
}
.chosen-container-multi .chosen-choices li.search-field input[type=text]{
	height:20px!important;
	font-size:14px;
}
/*Error Styles*/
#error_explanation ul{
	list-style:none;
	margin: 10px 0 20px 0;
	padding: 0;
}
#error_explanation li{
	background-color: #FF2626;
	padding: 5px 10px;
  margin: 0 0 10px 0;
	border-radius: 15px;
}
.field_with_errors input {
  border: 2px solid #ff2626;
}
a{
	color:#FF2626;
}
a:hover{
	text-decoration:none;
	color:#FFBF00;

}
a.active{
	color:#FFBF00;
}

/* Header styles */

header{
	position:fixed;
	top:0px;
	width:100%;
	z-index:9;
	height:105px;
}
.logobar{
	height:65px;
	position:fixed;
	top:0;
	width:100%;
	z-index:10;
	background-color:#000;
}
.logo{
	position:fixed;
	top:10px;
	left:20px;
	max-width:150px;
}
.promo-logo{
  position:relative;
  max-width:150px;
}
.search{
  position:fixed;
  right: 220px;
  top: 19px;
}
.social{
	position: fixed;
  right: 20px;
  top: 19px;
}
.social ul{
	list-style:none;
	padding:0;
	margin:0;
}
.social ul li{
	display:inline;

}
.social ul li:hover{
	-webkit-transition:0.4s all;
	transition:0.4s all;
}
.social ul li a, .social ul li a:last-child{
	padding: 3px;
	background-color: #999;
	-webkit-border-radius: 16px;
	-moz-border-radius: 16px;
	border-radius: 16px;
	display: inline-block;
	margin-left: 10px;
}
.social ul li a:hover{
	background-color: #FFBF00;
}
.social img{
	max-width:22px;
}


/*Main Navbar styles*/
.navbar {
	position: fixed;
	margin: 0;
	padding: 0;
	border: none;
	border-radius:0;
	background-image:url(<%= asset_path "nav-bg.png" %>);
	background-repeat:repeat;
	top:65px;
	width:100%;
	min-height:40px;
}
.navbar-default .navbar-collapse, .navbar-default .navbar-form {
  border: none;
  margin: 0;
  padding: 0 20px;
}

.navbar-default .navbar-nav > li > a {
  color: #FFF;
  border-right:1px solid #FFBF00;
  padding: 0 15px;
  line-height: 13px;
  font-size:13px;
  text-transform:uppercase;
}
.navbar-default .navbar-nav > li > a:hover, .navbar-default .navbar-nav > li > a.active {
  color: #FF2626;
}
.navbar-nav {
  margin: 14px 0 0 0;
}
.navbar-nav > li:first-child > a {
  padding-left:0;
}
.navbar-nav > li:last-child > a,  .navbar-nav > li.lastnav a {
  padding-right:0;
  border-right:none;
}
.login{
	float:right;
	margin:10px 0 0 0;
}
.login a{
	color:#FF2626;
	line-height:13px;
	font-size:13px;
	text-transform:uppercase;
}
.login a:hover{
	color:#FFBF00;
}
/*Backgrounds*/
.bg-full{
	background-size:cover;
	height:100%;
	width:100%;
	position: fixed;
	z-index:-1;
  top: 0;
  left: 0;
  background-position:top center;
}
#bg-home{
	background-image:url(<%= asset_path "bg-home.jpg" %>);
}
#bg-how-it-works{
	background-image:url(<%= asset_path "bg-how-it-works.jpg" %>);
}
#bg-pricing{
	background-image:url(<%= asset_path "bg-pricing.jpg" %>);
}
#bg-contact{
	background-image:url(<%= asset_path "bg-contact.jpg" %>);
}
#bg-about, #bg-general{
	background-image:url(<%= asset_path "bg-about.jpg" %>);
}
#bg-headshot {
  opacity: 0.4;
  -webkit-filter:grayscale(0.9);
  filter:grayscale(0.9);
}


/*Main Content*/
.main-content {
	width:100%;
	padding: 175px 0 100px 0;
}

.main-content-left{
	width:50%;
	float:left;
	padding-bottom: 100px;
}
.main-content-right{
	width:50%;
	float:right;
	padding-bottom: 100px;
}
.repeatable-row{
	padding-bottom:20px;
}
.subcontent-left  {
  width: 45%;
}
/*Search Results*/

.search-results .result{
	background-color:rgba(0,0,0,0.8) ;
	margin-bottom:20px;
	padding:20px;
	border-top: 1px solid #FF2626;
	border-bottom: 1px solid #FF2626;
	min-height: 165px;
}
.search-results .result img{
	height:125px;
}
.search-results .result h3{
	margin-top:0;
	margin-bottom:10px;
}
.search-results .result .btn{
	margin:10px 0 0 0;
}
.search-results .result .result-image{
	float:left;
	margin-right:20px;
}
/*Homepage Styles*/
.home-wrapper{
	position:fixed;
	top:45px;
	width:100%;
	background-color:#000000;
}
.home-col{
	float:left;
	width:19%;
	margin-right:1.25%;
}

.home-col-last{
	float:right;
	width:19%;
	margin:0;
}
.home-box{
	width:100%;
	margin:0 0 20px 0;
}
.home-box .img-responsive{
	-webkit-filter: grayscale(1);
	filter: grayscale(1);
}
.home-box .img-responsive:hover{
	-webkit-filter: grayscale(0);
	filter: grayscale(0);
	-webkit-transition:0.8s all;
	transition:0.8s all;
}
.home-box-half-left{
	float:left;
	width:48%;
}
.home-box-half-right{
	float:right;
	width:48%;
}
.home-box-calendar{
	position:relative;
}
.home-box-calendar .img-responsive{
	-webkit-filter: grayscale(0);
	filter: grayscale(0);
}
.home-box-calendar-text{
	position:absolute;
	top:0;
	left:0;
	width:100%;
	padding:25px;
}
.home-box-calendar-text h1{
	margin: 0 0 15px 0;
}
.home-box-calendar-text h3 {
	margin-top: 10px;
}
.home-box-calendar-text a{
	color:#FFF;
}
/*Dashboard Styles*/
.dash-title a{
	font-size:14px;
	line-height: 20px;
	width: 220px;
	text-align: center;
	float:right;
}

.resume-url span{
	color:#FFBF00;
}
.add-headshots, .add-videos {
  width: 100px;
  height: 100px;
  text-align: center;
  background-image:url(<%= asset_path "nav-bg.png" %>);
  background-repeat:repeat;
	padding-top: 20px;
}
.dashboard-headshot{
	width: 140px;
	float: left;
	margin-right: 10px;
	margin-bottom: 10px;
}
.dashboard-headshot img{
	height:105px
}
.dashboard-experience .row .col-md-4{
	width:100%;
	margin-bottom: 20px;
}
.new_headshot label, .new_video label{
	width:100%;
	margin-bottom: 0px;
}

#video_video_url{
	display:inline;
}

.video {
  display: inline;
}

.video-embed {
  display: inline-block;
  height: 145px;
  width: 258px;
}
.video-embed iframe,
.video-embed .video-js-box {
  height: 145px;
  width: 258px;
}

.provider-result .video-link > .play-button {
  position: relative;
  margin: 14px 0 0 24px;
}

.video-thumbnail {
  height: 125px
}
/*Edit Resume styles*/

.add-new-btn-wrapper{
	width:100%;
	margin: 20px 0 30px 0;
}
.radio-label{
  margin-left: 18px;
}
.radio-button{
  height: 20px;
}
.union-checkbox label {
  float: none;
  margin-right: 10px;
}
.union-checkbox input {
  margin-right: 5px;
}
.collection-checkbox {
  padding-top:  20px;
  padding-bottom:  20px;
}

.headshot{
	width: 19%;
	float: left;
	margin-right: 1%;
	margin-bottom: 20px;
}
label[for=resume_additional_skills] {
  margin-top: 1em;
}

.headshot-image{
	width:100%;
	padding-bottom: 10px;
}
.headshot-group{
	margin-bottom:20px;
}
.headshot-image img{
	width:100%;
}
.headshot-links, .video-links{
  margin-top: 1em;
	width:100%;
	font-size:12px;
}
.video{
	width: 24%;
	float: left;
	margin-right: 1.25%;
}

.video .cast-video{
	width:100%;
	height:150px;
}
.new_project, .new-school-form, .new-headshot-form, .new_video{
	padding-bottom:30px;
}
.project-list .col-md-4, .school-list .col-md-4 {
  padding-bottom: 30px;
}
.mini-link{
  font-size:12px;
}

/*Preview Resume Styles*/

h3.preview-descriptive{
	color: #FF2626;
	margin-top:0;
}

.skill-list {
  margin-top: 1.5em;
  width: 50%;
  float: left;
}

/* Footer styles */
footer{
	position:fixed;
	width:100%;
	height:40px;
	background-image:url(<%= asset_path "nav-bg.png" %>);
	background-repeat:repeat;
	bottom:0;
}
.footer-nav{
	list-style:none;
	padding:0;
	padding-top:10px;
}
.footer-nav li{
	display:inline;
}
.footer-nav li a{
	color: #CCC;
	border-right:1px solid #FFBF00;
	padding: 0 20px;
	line-height: 12px;
	font-size:12px;
}
.footer-nav li a.lastnav{
	border-right:none;
}
.footer-nav li a:hover{
	color:#FF2626;
}
#copyright{
	text-align:right;
	padding:13px 40px 0 0;
}
#copyright p{
	color:#CCC;
	font-size:11px;

}
.flash {
  width: 100%;
  margin: 0 auto 30px auto;
  padding: 15px;
  border-radius: 8px;
  margin-top: 0px;
}
.success {
  background: #FFBF00;
  border: 1px solid #000;
  color: #000;
}
.notice {
  background: #FFBF00;
  border: 1px solid #000;
  color: #000;
}
.failure, .warning {
  background: #FF2626;
  border: 1px solid #000;
  color: #fff;
}
/* Calendar Admin Styles */

.role{
  margin:20px 0 0 0;
}

/*Critique Request Styles */
.critique-request, .conversation, conversation-message{
  background-color:rgba(0,0,0,0.8) ;
  margin-bottom:20px;
	padding:20px;
	border-top: 1px solid #FF2626;
	border-bottom: 1px solid #FF2626;
}
.critique-request .critique-owner h3{
  color: #FFBF00;
	font-size: 28px;
	line-height: 35px;
}
@media (max-width: 1400px){
  .home-box-calendar-text h1 {
    font-size: 35px;
    line-height: 40px;
    margin: 0 0 10px 0;
  }

  .home-box-calendar-text img{
    width:30px;
    height:30px;
  }
  .home-box-calendar-text h3 {
    font-size:18px
  }
  .home-col {
    width: 24%;
    margin-right: 1%;
  }
}
@media (max-width: 1200px){
  h1 {
    font-size: 30px;
    line-height: 40px;
    margin: 0 0 20px 0;
  }
  .home-box-calendar-text{
    padding:15px
  }
  .home-box-calendar-text img{
    width:30px;
    height:30px;
  }
  .home-box {
    width: 100%;
    margin: 0 0 10px 0;
  }
  .home-col {
    width: 32%;
    margin-right: 1%;
  }
}
@media (max-width: 980px) {
  h1 {
    font-size: 35px;
    line-height: 45px;
    margin: 0 0 30px 0;
  }

  .home-box-calendar-text h3 {
    margin-top: 0px;
  }

  .main-content-right {
    width: 100%;
    float: right;
    padding-bottom: 60px;
  }

  .main-content-left {
    width: 100%;
    padding-bottom: 60px;
  }

  .main-content {
    width: 100%;
    padding: 120px 0 50px 0;
  }

  .navbar-default .navbar-toggle {
    background-color: #FF2626;
    border-color: #000;
  }

  .navbar-default .navbar-toggle .icon-bar {
    background-color: #FFF;
  }

  .navbar-default .navbar-toggle:hover, .navbar-default .navbar-toggle:focus {
    background-color: #FFBF00;
  }
}
@media (max-width: 767px){
  .logo {
    max-width: 100px;
    top: 17px;
    left:15px;
  }
  .social {
    right: 15px;
    top: 8px;
  }
  .social img {
    max-width: 17px;
  }
  .social ul li a, .social ul li a:last-child {
    padding: 0px 2px 2px 2px;
  }
  .search input[type="text"] {
    margin: 0 0 15px 0;
    padding: 1px 20px 2px 7px;
    width: 125px;
    font-size: 11px;
  }
  footer {
    height: 50px;
  }
  footer #copyright {
    text-align: center;
    padding: 5px 0;
    font-size: 10px;
  }
  footer .footer-nav {
    text-align: center;
    margin-bottom:0;
    padding-top:5px;
  }
  .footer-nav li a {
    padding: 0 10px;
    font-size: 10px;
  }
  .navbar-default .navbar-nav > li > a {
    border-right: none;
    padding: 0;
    line-height: 35px;
    font-size: 18px;
  }
  h1 {
    font-size: 35px;
    line-height: 45px;
    margin: 0 0 20px 0;
  }
  h2{
    font-size: 20px;
    padding-bottom:5px;
    margin-bottom:20px;
  }
  h3 {
    font-size: 20px;
    margin-bottom: 15px;
    margin-top: 15px;
  }
  h4 {
    font-weight:bold;
    font-size:15px;
    line-height: 16px;
    margin: 1em 0 10px 0;
  }
  .btn-primary, .btn, input[type="submit"] {
    padding: 5px 20px;
    margin: 10px 0 10px 0;
  }
  .login {
    float: left;
    margin: 10px 0 15px 0;
  }
  .resume-url{
    display:block;
    padding-left: 0px;
  }
  .home-box-calendar-text {
    padding: 7px;
  }
  .home-box-calendar-text h1 {
    font-size: 24px;
    line-height: 24px;
    margin: 0 0 5px 0;
  }
  .home-box-calendar-text img{
    width:20px;
    height:20px;
  }
  .home-box-calendar-text h3 {
    font-size:14px
  }
}
@media (max-width: 480px){
  header{
    height:90px;
  }
  .logobar{
    height:40px;
  }
  .navbar {
    top:40px
  }
  .navbar-toggle {
    padding: 7px 7px;
    margin-top: 5px;
    margin-right: 5px;
    margin-bottom: 5px;
  }
  .home-box-calendar-text h1 {
    font-size: 18px;
    line-height: 18px;
    margin: 0 0 5px 0;
  }

  .home-box-calendar-text img{
    width:16px;
    height:16px;
  }
  .home-box-calendar-text h3 {
    font-size:11px;
    margin-bottom:3px;
  }
  .logo {
    max-width: 80px;
    top: 8px;
    left:5px;
  }
  .social {
    right: 5px;
    top: 10px;
  }
  .social img {
    max-width: 16px;
  }
  .social ul li a, .social ul li a:last-child {
    padding: 0px 2px 2px 2px;
    margin-left: 5px;
  }
  .search {
    right: 90px;
    top: 12px;
  }
  .search input[type="text"] {
    margin: 0 0 15px 0;
    padding: 1px 10px 2px 5px;
    width: 90px;
    font-size: 10px;
  }
}
@media (max-width: 380px){

}
.chosen-container {
  color: #444;
  width: 300px;
}
.chosen-container a {
  transition: none;
}
.chosen-container .chosen-choices {
  width: 300px;
}

.promo-or-log-in span {
  padding: 0 6px;
}

.critique-notes {
  padding: 20px 5px 5px 5px;
  display: block;
}
.critique-payment {
  color: #FFBF00;
}