<div class="row repeatable-row">
  <% schools.each do |school| %>
    <div class="col-md-12">
      <p>Acting: <%= school.acting %></p>
      <p>Dance: <%= school.dance %></p>
      <p>Voice: <%= school.voice %></p>
      <p>Instruments: <%= school.instruments %></p>
      <p>Stage Combat: <%= school.stage_combat %></p>
    </div>
  <% end %>
</div>
