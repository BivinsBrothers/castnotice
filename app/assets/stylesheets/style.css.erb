@charset "iso-8859-1";
 #video-disc{
                        display:none;
                    }
@-moz-document url-prefix() {
                        #video-disc{
                        display:block;
                    }
                    }
/* CSS Document #FF2626 (red) #FFBF00 (yellow)*/
html {
  background-color:#000;
}
html, body{
	height:100%;
	width:100%;
	font-size:14px;
	color:#FFF;
	font-family: 'Open Sans', sans-serif;
}
body{
  background-color:transparent;
}
h1, h2, h3, h4, h5, h6, p{
	-webkit-font-smoothing: antialiased;
	font-family: 'Open Sans', sans-serif;
}
h1{
	font-weight:300;
	color:#FFF;
	font-size:55px;
	line-height: 55px;
	margin:0 0 30px 0;
	padding:0;

}
h1.pricing{
	margin:0 0 10px 0;
}
h2{
	font-size: 22px;
	color: #FF2626;
	font-weight:300;
	padding-bottom:5px;
	margin-bottom:30px;
	border-bottom:1px solid #FF2626;
}
h3{
	font-size: 22px;
	color: #FFBF00;
	font-weight:300;
	margin-bottom: 20px;
}
h4{
	color:#fff;
	font-weight:bold;
	font-size:16px;
	line-height: 16px;
	margin: 0 0 12px 0;
}
.glyphicon {
  top: 2px;
  color:#FF2626;
}
ol.how-it-works{
	counter-reset: li;
	list-style: none;
	padding-left:30px;
}
ol li {
	position: relative;
	display: block;
	padding: 0px 0 3px 20px;
	margin: 0 0 20px 0;
	font-size: 14px;
}
ol.how-it-works li:before {
	content: counter(li);
	counter-increment: li;
	position: absolute;
	left: -25px;
	top: 0;
	margin-top: 0;
	background: #FF2626;
	height: 30px;
	width: 30px;
	line-height: 25px;
	text-align: center;
	padding: 2px 2px 1px 0px;
	border-radius: 2em;
	font-size: 20px;
	color: #FFF;
}
/*Form Styles*/
.btn, a{
	-webkit-transition:0.8s all;
	transition:0.8s all;
}
input, select, textarea, input[type="checkbox"]{
	margin: 0 0 15px 0;
	color: #444;
	background-color:#EEE;
	font-family: 'Open Sans', sans-serif;
	border: none;
	border-radius: 7px;
	padding: 4px 20px 4px 7px;
	display: block;

}

input:focus, textarea:focus, isindex:focus, keygen:focus, select:focus {
	outline:none;
	background-color:#FFF;
}
input[type="checkbox"]{
		margin: 0 0 10px 0;
	color: #444;
	background-color:#EEE;
	font-family: 'Open Sans', sans-serif;
	border: none;
	border-radius: 5px;
	padding: 3px 15px;
	display: inline;

}
input[type="file"]{
	background-color: transparent;
	display: inline;
	color:#fff;
	padding: 0;
}
.btn-primary, .btn, input[type="submit"] {
  	font-family: 'Open Sans', sans-serif;
	color:#fff;
	background-color: #FF2626;
	border: none;
	border-radius: 20px;
	padding: 10px 30px;
	margin: 15px 0 15px 0;
	display: inline-block;
}
.btn-small{
	font-size: 12px;
	padding: 0px 10px 1px 10px;
	margin: 0 0 0 10px;
}
.btn-add-new{
	font-size: 14px;
	color: #FF2626;
}
.btn-edit{
	font-size: 14px;
	color: #FF2626;
}
.btn:hover, .btn:focus, input[type="submit"]:hover, .btn-primary:hover {
	outline: none;
	background-color: #FFBF00;
	background-position: 0 -15px;
	border: none;
}

label {
	display: inline;
	margin-bottom: 2px;
	font-weight: normal;
	float: left;
	width: 200px;
}

.is-hidden {
  display:none;
}

#user_birthday_1i, #user_birthday_2i, #user_birthday_3i{
	display:inline;
}

/*Error Styles*/
#error_explanation ul{
	list-style:none;
	margin: 10px 0 20px 0;
	padding:0;
}
#error_explanation li{
	background-color: #FF2626;
	padding: 5px 10px;
	border-radius: 15px;
}
a{
	color:#FF2626;
}
a:hover{
	text-decoration:none;
	color:#FFBF00;

}
a.active{
	color:#FFBF00;
}

/* Header styles */

header{
	position:fixed;
	top:0px;
	width:100%;
	z-index:9;
	height:105px;
}
.logobar{
	height:65px;
	position:fixed;
	top:0;
	width:100%;
	z-index:10;
	background-color:#000;
}
.logo{
	position:fixed;
	top:10px;
	left:20px;
	max-width:150px;
}
.social{
	position: fixed;
right: 20px;
top: 19px;
}
.social ul{
	list-style:none;
	padding:0;
	margin:0;
}
.social ul li{
	display:inline;

}
.social ul li:hover{
	-webkit-transition:0.4s all;
	transition:0.4s all;
}
.social ul li a, .social ul li a:last-child{
	padding: 3px;
background-color: #999;
-webkit-border-radius: 16px;
-moz-border-radius: 16px;
border-radius: 16px;
display: inline-block;
margin-left: 10px;
}

.social img{
	max-width:22px;
}


/*Main Navbar styles*/
.navbar {
	position: fixed;
	margin: 0;
	padding: 0;
	border: none;
	border-radius:0;
	background-image:url(<%= asset_path "nav-bg.png" %>);
	background-repeat:repeat;
	top:65px;
	width:100%;
	min-height:40px;
}
.navbar-default .navbar-collapse, .navbar-default .navbar-form {
border: none;
margin: 0;
padding: 0 20px;
}

.navbar-default .navbar-nav > li > a {
color: #FFF;
border-right:1px solid #FFBF00;
padding: 0 15px;
line-height: 13px;
font-size:13px;
text-transform:uppercase;
}
.navbar-default .navbar-nav > li > a:hover, .navbar-default .navbar-nav > li > a.active {
color: #FF2626;
}
.navbar-nav {
margin: 14px 0 0 0;
}
.navbar-nav > li:first-child > a {
padding-left:0;
}
.navbar-nav > li:last-child > a,  .navbar-nav > li.lastnav a {
padding-right:0;
border-right:none;
}
.login{
	float:right;
	margin:10px 0 0 0;
}
.login a{
	color:#FF2626;
	line-height:13px;
	font-size:13px;
	text-transform:uppercase;
}
.login a:hover{
	color:#FFBF00;
}
/*Backgrounds*/
.bg-full{
	background-size:cover;
	height:100%;
	width:100%;
	position: fixed;
	z-index:-1;
  top: 0;
  left: 0;
  background-position:top right;
}
#bg-home{
	background-image:url(<%= asset_path "bg-home.jpg" %>);
}
#bg-how-it-works{
	background-image:url(<%= asset_path "bg-how-it-works.jpg" %>);
}
#bg-pricing{
	background-image:url(<%= asset_path "bg-pricing.jpg" %>);
}
#bg-contact{
	background-image:url(<%= asset_path "bg-contact.jpg" %>);
}
#bg-about, #bg-general{
	background-image:url(<%= asset_path "bg-about.jpg" %>);
}
#bg-headshot {
  opacity: 0.4;
  -webkit-filter:grayscale(0.9);
  filter:grayscale(0.9);
}


/*Main Content*/
.main-content {
	width:100%;
	padding: 200px 0 100px 0;
}

.main-content-left{
	width:50%;
	float:left;
	padding-bottom: 100px;
}
.main-content-right{
	width:50%;
	float:right;
	padding-bottom: 100px;
}
.repeatable-row{
	padding-bottom:20px;
}
.subcontent-left  {
  width: 45%;
}
/*Homepage Styles*/
.home-wrapper{
	position:fixed;
	top:45px;
	width:100%;
	height:100%;
	background-color:#000000;	
}
.home-col{
	float:left;
	width:19%;
	margin-right:1.25%;	
}

.home-col-last{
	float:right;
	width:19%;
	margin:0;	
}
.home-box{
	width:100%;
	margin:0 0 20px 0;	
}
.home-box .img-responsive{
	-webkit-filter: grayscale(1);
	filter: grayscale(1);	
}
.home-box .img-responsive:hover{
	-webkit-filter: grayscale(0);
	filter: grayscale(0);	
	-webkit-transition:0.8s all;
	transition:0.8s all;
}
.home-box-half-left{
	float:left;
	width:48%;	
}
.home-box-half-right{
	float:right;
	width:48%;	
}
.home-box-calendar{
	position:relative;
}
.home-box-calendar .img-responsive{
	-webkit-filter: grayscale(0);
	filter: grayscale(0);
}
.home-box-calendar-text{
	position:absolute;
	top:0;
	left:0;
	width:100%;
	padding:25px;
}
.home-box-calendar-text h1{
margin: 0 0 15px 0;
}
.home-box-calendar-text h3 {
margin-top: 10px;
}
.home-box-calendar-text a{
	color:#FFF;
}
/*Dashboard Styles*/
.dash-title a{
	font-size:14px;
	line-height: 20px;
	width: 220px;
	text-align: center;
	float:right;
}
.add-headshots, .add-videos {
    width: 100px;
    height: 100px;
    text-align: center;
    background-image:url(<%= asset_path "nav-bg.png" %>);
    background-repeat:repeat;
	padding-top: 20px;
}
.new_headshot label, .new_video label{
	width:100%;
	margin-bottom: 0px;
}

#video_video_url{
	display:inline;
}


.video {
  display:inline;
}

.provider-result .video-link > .play-button {
  position: relative;
  margin: 14px 0 0 24px;
}

.video-thumbnail {
  height: 125px
}
/*Edit Resume styles*/

.add-new-btn-wrapper{
	width:100%;
	margin: 20px 0 30px 0;
}
.radio-label{
  margin-left: 18px;
}
.radio-button{
  height: 20px;
}
.union-checkbox label {
   float: none;
}
.union-checkbox input {
  margin-left: 20px;
}
.headshot{
	width:9%;
	float:left;
	margin-right:1%;
}
.headshot-image{
	width:100%;
	padding-bottom: 5px;
}
.headshot-links, .video-links{
	width:100%;
	font-size:12px;
}
.video{
	width: 24%;
	float: left;
	margin-right: 1.25%;
}

.video .cast-video{
	width:100%;
	height:150px;
}
.new_project, .new-school-form, .new-headshot-form, .new_video{
	padding-bottom:30px;
}
.project-list .col-md-4, .school-list .col-md-4 {
  padding-bottom: 30px;
}
.mini-link{
  font-size:12px;
}

/* Footer styles */
footer{
	position:fixed;
	width:100%;
	height:40px;
	background-image:url(<%= asset_path "nav-bg.png" %>);
	background-repeat:repeat;
	bottom:0;
}
.footer-nav{
	list-style:none;
	padding:0;
	padding-top:10px;
}
.footer-nav li{
	display:inline;
}
.footer-nav li a{
	color: #CCC;
	border-right:1px solid #FFBF00;
	padding: 0 20px;
	line-height: 12px;
	font-size:12px;
}
.footer-nav li a.lastnav{
	border-right:none;
}
.footer-nav li a:hover{
color:#FF2626;
}
#copyright{
	text-align:right;
	padding:13px 40px 0 0;
}
#copyright p{
	color:#CCC;
	font-size:11px;

}
.flash {
  width: 80%;
  max-width: 900px;
  margin: 30px auto;
  padding: 15px;
  border-radius: 8px;
  margin-top: 125px;
}
.success {
  background: #e1f1e4;
  border: 1px solid #cbf4ca;
  color: #53b060;
}
.notice {
  background: #e1f1f9;
  border: 1px solid #caecf4;
  color: #003080;
}
.failure, .warning {
  background: #f4e5e4;
  border: 1px solid #f0dcdf;
  color: #c5625d;
}

