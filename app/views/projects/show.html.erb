<% current_user.projects.each do |project| %>
  <ul>
    <li>Project Type: <%= project_type_name(project.project_type) %></li>
    <li>Title: <%= project.title %></li>
    <li>Role: <%= project.role %></li>
    <li>Director Studio: <%= project.director_studio %></li>
    <li><%= link_to 'Edit', edit_project_path(project), class: "btn" %></li>
  </ul>
<% end %>
